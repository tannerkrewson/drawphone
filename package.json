{
	"name": "drawphone",
	"version": "1.0.0",
	"description": "Telephone with pictures",
	"private": true,
	"scripts": {
		"start": "run-s build:prod start:prod",
		"dev": "run-p start:dev build:dev",
		"start:dev": "nodemon --exec npm run start:prod --ignore public/",
		"build:dev": "webpack --mode development --watch",
		"start:prod": "node ./server/bin/www",
		"build:prod": "webpack --mode production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tannerkrewson/drawphone.git"
	},
	"author": "Tanner Krewson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tannerkrewson/drawphone/issues"
	},
	"homepage": "https://github.com/tannerkrewson/drawphone#readme",
	"dependencies": {
		"blueimp-canvas-to-blob": "^3.27.0",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.5.0",
		"bootstrap-sweetalert": "^1.0.1",
		"cookie-parser": "^1.4.4",
		"css-loader": "^3.6.0",
		"debug": "^4.1.1",
		"dexie": "^3.0.1",
		"express": "^4.17.1",
		"fabric": "^3.6.3",
		"jquery": "^3.5.1",
		"knuth-shuffle": "^1.0.8",
		"mini-css-extract-plugin": "^0.9.0",
		"morgan": "^1.9.1",
		"npm-run-all": "^4.1.5",
		"popper.js": "^1.16.1",
		"pug": "^2.0.4",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.0",
		"striptags": "^3.1.1",
		"typeface-slabo-27px": "0.0.72",
		"url-loader": "^4.1.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"devDependencies": {
		"husky": "^3.0.2",
		"nodemon": "^2.0.3",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
